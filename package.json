    "lint:lib:sh-ui": "nx lint sh-ui --skip-nx-cache",
    "lint:lib:sh-ui:fix": "nx lint sh-ui --fix --skip-nx-cache",
    "test:lib:sh-ui": "nx test sh-ui --skip-nx-cache",
    "test:lib:sh-ui:watch": "nx test sh-ui --watch --skip-nx-cache",
    "build:lib:sh-ui": "nx build sh-ui",
    "build:lib:sh-ui:force": "nx build sh-ui --skip-nx-cache",
    "build:lib:sh-ui:dev": "nx build sh-ui --configuration=development",
    "serve:cl-main": "nx serve cl-main --configuration=development",
    "build:cl-main": "cross-env NODE_ENV=production nx build cl-main --configuration=production",
    "build:cl-main:force": "cross-env NODE_ENV=production nx build cl-main --configuration=production --skip-nx-cache",
    "start:cl-main": "nx start cl-main --configuration=production",
    "lint:cl-main": "nx lint cl-main",
    "lint:cl-main:fix": "nx lint cl-main --fix",
    "test:cl-main": "nx test cl-main",
    "test:cl-main:watch": "nx test cl-main --watch",
    "test:cl-main:coverage": "nx test cl-main --coverage",
    "prepare": "husky",
    "git:prepare-hooks": "husky",
    "lint:all": "nx run-many -t lint --all",
    "lint:all:fix": "nx run-many -t lint --all --fix",
    "build:all:production": "nx run-many -t build --all --configuration=production",
    "ci:validate:affected": "cross-env NODE_ENV=production pnpm run format:check && pnpm run lint:affected:fix && pnpm run test:affected --coverage && pnpm run build:affected:prod",
    "ci:validate:all": "cross-env NODE_ENV=production pnpm run format:check && pnpm run lint:all:fix && pnpm run test:all:coverage && pnpm run build:all:prod --skip-nx-cache",
    "db:migration:generate": "ts-node -r tsconfig-paths/register -P ./apps/api-principal/tsconfig.tools.json ./node_modules/typeorm/cli.js -d ./apps/api-principal/src/config/typeorm-cli.config.ts migration:generate",
    "lint-staged:run-eslint": "eslint --max-warnings=0 --fix --config ./eslint.config.mjs",
    "lint-staged:nx-format": "cross-env NX_DAEMON=false pnpm exec nx format:write --files"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "pnpm run lint-staged:run-eslint"
    ],
    "*.{js,jsx,ts,tsx,mjs,cjs,json,md,html,css,scss,yaml,yml}": [
      "pnpm run lint-staged:nx-format"
    ]
  },
